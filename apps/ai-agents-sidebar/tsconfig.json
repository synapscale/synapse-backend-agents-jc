      "../../../components/sidebar/*": [
        "../../../components/sidebar/*"
      ],
      "../../../packages/theme/*": [
        "../../../packages/theme/*"
      ]
    },
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "**/*",
    "../../../components/**/*",
    "../../../packages/**/*",
    "../../../shared/node-sidebar/node-sidebar.tsx",
    "../../../shared/node-sidebar/**/*",
    "components/ui/section.tsx",
    "components/ui/card.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
